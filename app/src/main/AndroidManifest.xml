<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project.cm.mediatracker">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".ListMediaActivity" />
        <activity android:name=".SearchMediaActivity" />
        <activity android:name=".PerfilActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".MovieActivity" />
        <activity android:name=".SerieAnimeActivity" />
        <activity android:name=".LoginActivity"></activity>
    </application>

</manifest>